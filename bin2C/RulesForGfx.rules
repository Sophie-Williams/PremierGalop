<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Conversion"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="ConvertBinToCArray"
			DisplayName="Convert binary files to C arrays"
			CommandLine="&quot;$(SolutionDir)$(ConfigurationName)\bin2C.exe&quot; [Inputs]"
			Outputs="$(InputName).h"
			FileExtensions="*.png;*.ttf"
			ExecutionDescription="Convert $(InputName) to C Array File"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
